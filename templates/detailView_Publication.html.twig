                     {% elseif value['title'] %}    
                         <a href="{{ value['uri'] }}">{{ value['title'] }}</a>
                     {% elseif value['uri'] %}
                         <!-- if the contributor has no acdh id but other 3rd party id, then we will not use the oeaw_detail page -->
                         <a href="{{ value['uri'] }}">{{ value['uri'] }}</a>
                     {% else %}
                         {{ value }},
                     {% endif%}
                     <span class="res-prop-delimiter">,</span>
                {% endfor %}
            </span>
        </div>                
    {% endif%}	
    
